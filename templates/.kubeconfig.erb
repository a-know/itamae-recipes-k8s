apiVersion: v1
clusters:
- cluster:
    server: http://<%= @master_priv_ip %>:8080
  name: local-server
contexts:
- context:
    cluster: local-server
    namespace: default
    user: myself
  name: default-context
current-context: default-context
kind: Config
preferences: {}
users:
- name: myself
  user:
    password: <%= @kubectl_admin_password %>
    username: admin
